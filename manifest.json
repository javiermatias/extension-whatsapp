{
  "manifest_version": 3,
  "name": "Google Messages Bulk Sender",
  "version": "1.0",
  "description": "Sends a message to a list of numbers on messages.google.com.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "options_page": "data.html",
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  }, 
  "content_scripts": [
    {
      "matches": ["*://messages.google.com/web/*"],
      "js": ["content.js"]
    }
  ]

}